{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.debugSdkLibraries": false,
  "dart.lineLength": 120,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.rulers": [120],
    "files.insertFinalNewline": true
  },
  "files.exclude": {
    "**/LICENSE": true,
    "**/CHANGELOG.md": true,
    "**/README.md": true,
    "**/.dart_tool": true,
    "**/.metadata": true,
    "**/.idea": true,
    "**/*.iml": true,
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "search.exclude": {
    "**/.fvm": true
  },
}
